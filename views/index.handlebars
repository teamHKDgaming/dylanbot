<!-- features section -->
<section id="features" class="lb-bg-white">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center font-weight-light text-uppercase my-5">
                <h1>{{lang.website_home_features}}</h1>
            </div>
            <div class="col-md-6 col-lg-4 mb-4 text-center">
                <div class="card lb-feature-card shadow">
                    <div class="card-body">
                        <i class="mdi mdi-console-line lb-feature-icon"></i>
                        <h5 class="card-title">{{lang.website_home_features_commands}}</h5>
                        <hr>
                        <p class="card-text">{{lang.website_home_features_commands_description}}</p>
                    </div>
                    <div class="card-footer lb-card-footer mb-2">
                        <a href="/commands" class="card-link">{{lang.website_home_features_commands_link}}</a>

                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-4 text-center">
                <div class="card lb-feature-card shadow">
                    <div class="card-body">
                        <i class="mdi mdi-settings lb-feature-icon"></i>
                        <h5 class="card-title">{{lang.website_home_features_configurable}}</h5>
                        <hr>
                        <p class="card-text">{{lang.website_home_features_configurable_description}}</p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-4 text-center">
                <div class="card lb-feature-card shadow">
                    <div class="card-body">
                        <i class="mdi mdi-folder-multiple lb-feature-icon"></i>
                        <h5 class="card-title">{{lang.website_home_features_allinone}}</h5>
                        <hr>
                        <p class="card-text">{{lang.website_home_features_allinone_description}}</p>
                    </div>
                    <div class="card-footer lb-card-footer mb-2">
                        <a href="/commands" class="card-link">{{lang.website_home_features_allinone_link}}</a>

                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-4 text-center">
                <div class="card lb-feature-card shadow">
                    <div class="card-body">
                        <i class="mdi mdi-currency-usd-off lb-feature-icon"></i>
                        <h5 class="card-title">{{lang.website_home_features_free}}</h5>
                        <hr>
                        <p class="card-text">{{lang.website_home_features_free_description}}</p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-4 text-center">
                <div class="card lb-feature-card shadow">
                    <div class="card-body">
                        <i class="mdi mdi-chart-line lb-feature-icon"></i>
                        <h5 class="card-title">{{lang.website_home_features_dashboard}}</h5>
                        <hr>
                        <p class="card-text">{{lang.website_home_features_dashboard_description}}</p>
                    </div>
                    <div class="card-footer lb-card-footer mb-2">
                        {{#if user}}
                        <a href="/servers" class="card-link">{{lang.website_home_features_dashboard_servers}}</a>
                        {{ else }}
                        <a href="/loginpressedbutton"
                            class="card-link">{{lang.website_home_features_dashboard_link}}</a>
                        {{/if}}
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 mb-4 text-center">
                <div class="card lb-feature-card shadow">
                    <div class="card-body">
                        <i class="mdi mdi-cloud-check lb-feature-icon"></i>
                        <h5 class="card-title">{{lang.website_home_features_uptime}}</h5>
                        <hr>
                        <p class="card-text">{{lang.website_home_features_uptime_description}}</p>
                    </div>
                    <div class="card-footer lb-card-footer mb-2">
                        <a href="https://status.lenoxbot.com"
                            class="card-link">{{lang.website_home_features_uptime_statuspage}}</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- /feature section -->
<!-- What our customers say -->
<section id="rating" class="lb-bg-white">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center font-weight-light text-uppercase my-5">
                <h1>{{lang.website_home_rating_title}} ‚ù§</h1>
            </div>
            <div class="col-12 text-center" style="margin-bottom: 30px;">
                <div id="lenoxbotrating" class="carousel slide" data-ride="carousel">
                    <div class="carousel-inner">
                        {{#each ratings}}
                        <div class="carousel-item p-4 text-center rating-{{@index}}">
                            <div class="card shadow">
                                <div class="card-body">
                                    <h4 class="card-title"><i class="mdi mdi-star lb-starchecked"></i><i
                                            class="mdi mdi-star lb-starchecked"></i><i
                                            class="mdi mdi-star lb-starchecked"></i><i
                                            class="mdi mdi-star lb-starchecked"></i><i
                                            class="mdi mdi-star lb-starchecked"></i></h4>
                                    <blockquote class="card-text lb-blockquote m-4">
                                        {{quote}}
                                        <cite class="lb-cite">{{cite}}</cite>
                                    </blockquote>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                    <a class="carousel-control-prev" href="#lenoxbotrating" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon lb-rating-previcon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#lenoxbotrating" role="button" data-slide="next">
                        <span class="carousel-control-next-icon lb-rating-nexticon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>
<script>
    $('.rating-0').addClass('active')
</script>
<!-- lenoxbot index JS -->
<script src="js/index.js"></script>